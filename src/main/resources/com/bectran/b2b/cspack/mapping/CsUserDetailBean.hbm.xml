<?xml version="1.0"?>
	<!DOCTYPE hibernate-mapping PUBLIC
	        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>  
		<class name="com.bectran.b2b.cspack.beans.CsUserDetailBean" table="s_user">
	  		
	    	<id name="userId" type="string" column="UserId">
				<generator class="assigned" />
			</id>  

			<property name="firstName" type="string" column="FName" />
			<property name="lastName" type="string" column="LName" />

			<property name="opsAdministrator" type="string" column="opsAdministrator" />
			<property name="internalCode" type="string" column="internalCode" />

	
	    </class>
	
	</hibernate-mapping>
	
