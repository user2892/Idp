<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>  
	<class name="com.bectran.b2b.enterprise.beans.CustomerDataUpdateStatusByGroupBean" table="zs_data_load_status_group">
      	<id name="id" type="string" column="id">
			<generator class="assigned" />
		</id>  
		
	<property name="supplierId" type="long" column="supplierId" />	
	<property name="startTime" type="timestamp" column="startTime" />	
	<property name="endTime" type="timestamp" column="endTime" />		
	<property name="numberOfRec" type="integer" column="numberOfRec" />
	<property name="numberOfBatch" type="integer" column="numberOfBatch" />
	<property name="cusDataType" type="string" column="cusDataType" />
	
	<property name="groupId" type="long" column="groupId" />
	
    </class>

</hibernate-mapping>

