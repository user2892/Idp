<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.bectran.b2b.supplier.beans.GroupCustomAppCntrlBean" table="s_group_custom_cntrl">
		<id name="id" type="string" column="id">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="long" column="groupId" />	
		<property name="formType" type="string" column="formType" />
		
		<property name="status" type="string" column="status" />
		<property name="name" type="string" column="name" />
		
		<property name="page1Status" type="string" column="page1Status" />
		<property name="page2Status" type="string" column="page2Status" />


	</class>

</hibernate-mapping>

