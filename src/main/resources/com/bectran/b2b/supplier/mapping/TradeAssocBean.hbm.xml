<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.bectran.b2b.supplier.beans.TradeAssocBean" table="s_tradeassoc">
     	<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		<property name="firstName" type="string" column="FName" />
		<property name="lastName" type="string" column="LName" />
		<property name="phone" type="string" column="PhoneNbr" />
		<property name="email" type="string" column="EMail" />
		<property name="fax" type="string" column="FaxNbr" />
		<property name="lastUpdate" type="timestamp" column="LastUpdate" />	

 		<property name="addressLineOne" type="string" column="addressLineOne" />
 		<property name="addressLineTwo" type="string" column="addressLineTwo" />
 		<property name="city" type="string" column="city" />
 		<property name="state" type="string" column="state" />
 		<property name="zipCode" type="string" column="zipCode" />
 		<property name="countryId" type="string" column="countryId" />
 		
 		<property name="status" type="string" column="status" />
 		<property name="assocName" type="string" column="assocName" />
 		<property name="supplierId" type="long" column="supplierId" />			
 		
        				
    </class>

</hibernate-mapping>