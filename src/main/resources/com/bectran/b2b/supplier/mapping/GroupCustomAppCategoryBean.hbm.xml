<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.bectran.b2b.supplier.beans.GroupCustomAppCategoryBean" table="s_group_custom_content_cat">
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		<property name="groupId" type="long" column="groupId" />	
		<property name="name" type="string" column="name" />	
		<property name="displayOrder" type="integer" column="displayOrder" />
		
		<property name="status" type="string" column="status" />	
		<property name="code" type="string" column="code" />
        
	</class>

</hibernate-mapping>

