<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.bectran.b2b.customer.beans.AppCustomerContactBean" table="c_app_customer_contact">
     	<id name="id" type="long" column="id">
			<generator class="native" />
		</id>

 		<property name="acctNum" type="string" column="AcctNum" />
        <property name="name" type="string" column="Name" />
        <property name="dateCreated" type="timestamp" column="DateCreated" />
      	<property name="addressLineOne" type="string" column="addressLineOne" />
		<property name="addressLineTwo" type="string" column="addressLineTwo" />
		<property name="city" type="string" column="city" />
		<property name="countryId" type="string" column="countryId" />
		<property name="state" type="string" column="state" />
		<property name="zipCode" type="string" column="zipCode" />		
 		<property name="orgMainPhone" type="string" column="orgMainPhone" />										
  		<property name="orgMainFax" type="string" column="orgMainFax" />	   	
  		
		<property name="requestId" type="long" column="requestId" />	
  		<property name="customerDBAName" type="string" column="customerDBAName" />	
  		
    </class>

</hibernate-mapping>
