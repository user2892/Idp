<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bectran.b2b.customer.beans.CreditReqTransmitDetailsBean"
        table="c_creditapp_transmit_details">
        <id name="headerId" type="long" column="headerId">
            <generator class="assigned" />
        </id>
        <property name="transmitEnabled" type="string" column="transmitEnabled" />
        <property name="transmitStatus" type="string" column="transmitStatus" />
        <property name="transmitDate" type="timestamp" column="transmitDate" />
        <property name="transmitMode" type="string" column="transmitMode" />
    </class>
</hibernate-mapping>