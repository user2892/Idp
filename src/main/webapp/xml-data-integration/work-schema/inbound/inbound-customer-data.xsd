			<bectran:element name="customerLegalName">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="300" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="dateCreatedInBectran" type="bectran:date">
