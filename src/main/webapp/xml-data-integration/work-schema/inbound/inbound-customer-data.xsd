<?xml version="1.0" encoding="UTF-8"?>
<bectran:schema targetNamespace="http://www.bectran.com/"
	elementFormDefault="qualified" xmlns:bectran="http://www.w3.org/2001/XMLSchema"
	xmlns="http://www.bectran.com/">

	<bectran:element name="inbound-customer-data" type="inbound-customer-data-type"></bectran:element>

	<bectran:complexType name="inbound-customer-data-type">
		<bectran:sequence>
			<bectran:element name="customer" type="customer-type"
				maxOccurs="1"></bectran:element>
		</bectran:sequence>
	</bectran:complexType>

	<bectran:complexType name="customer-type">
		<bectran:sequence>
			<bectran:element name="business-group-info" type="business-group-info-type" minOccurs="0" maxOccurs="1"/>
			<bectran:element name="basic-info" type="basic-info-type"	maxOccurs="1" />
			
			<bectran:element name="tax-exempt-info" type="tax-exempt-info-type" minOccurs="0" maxOccurs="5" />
						
			<bectran:element name="sales-person-info" type="sales-person-info-type" minOccurs="0"  />					
			<bectran:element name="operations-unit-info" type="operations-unit-info-type" minOccurs="0" />				
			<bectran:element name="owner-officer-info" type="owner-officer-info-type" minOccurs="0" maxOccurs="5" />
			<bectran:element name="billing-info" type="billing-info-type" minOccurs="0" maxOccurs="10" />
			<bectran:element name="shipping-info" type="shipping-info-type" minOccurs="0" maxOccurs="10" />
			<bectran:element name="apay-info" type="apay-info-type" minOccurs="0" maxOccurs="1" />
			<bectran:element name="page-2-info" type="page-2-info-type" minOccurs="0" />
            <bectran:element name="credit-reference-info" type="credit-reference-info-type" minOccurs="0" />
            <bectran:element name="custom-doc-info" type="custom-doc-info-type" minOccurs="0" />
            <bectran:element name="credit-request-info" type="credit-request-info-type" minOccurs="1" maxOccurs="1"/>
		</bectran:sequence>

		<bectran:attribute name="account-id" use="required">
			<bectran:simpleType> 
				<bectran:restriction base="bectran:string">
					<bectran:minLength value="1"></bectran:minLength>
					<bectran:maxLength value="25"></bectran:maxLength>
				</bectran:restriction>
			</bectran:simpleType>
		</bectran:attribute>
		
		<bectran:attribute name="transaction-id" use="required">
			<bectran:simpleType> 
				<bectran:restriction base="bectran:string">
					<bectran:minLength value="1"></bectran:minLength>
					<bectran:maxLength value="25"></bectran:maxLength>
				</bectran:restriction>
			</bectran:simpleType>
		</bectran:attribute>		
		
	</bectran:complexType>

	<bectran:complexType name="business-group-info-type">
		<bectran:sequence>
			<bectran:element name="groupName">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="1"/>
						<bectran:maxLength value="100"/>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="internalGroupCode">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"/>
						<bectran:maxLength value="45"/>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="bectranReferenceId">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="1"/>
						<bectran:maxLength value="45"/>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>			
		</bectran:sequence>
	</bectran:complexType>


	<bectran:complexType name="basic-info-type">
		<bectran:sequence>
			<bectran:element name="addressLineOne">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
