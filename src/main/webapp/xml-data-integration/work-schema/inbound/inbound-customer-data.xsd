				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="opsUnitCode">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="100" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>		
		</bectran:sequence>
	</bectran:complexType>
	
	
<!-- TAX EXEMPT INFORMATION -->
	<bectran:complexType name="tax-exempt-info-type">
		<bectran:sequence>
			<bectran:element name="entry"
				type="tax-exempt-info-entry-type" maxOccurs="unbounded" />
		</bectran:sequence>
	</bectran:complexType>
	
	<bectran:complexType name="tax-exempt-info-entry-type">
		<bectran:sequence>
			<bectran:element name="certNum">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="50" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="stateCode">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="50" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="expiration">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="40" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="hasExpirationDate">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="20" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="docImageUrl">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="1000" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			
		</bectran:sequence>
	</bectran:complexType>	

