			<bectran:element name="federalTaxId">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="20"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="numOfEmployee">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="100"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="phone">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="40"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="state">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="1"></bectran:minLength>
						<bectran:maxLength value="100"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="stateOfIncorporation">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="100"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="styleOfBusiness">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="1"></bectran:minLength>
						<bectran:maxLength value="200"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>


			<bectran:element name="typeOfBusiness">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="1"></bectran:minLength>
						<bectran:maxLength value="200"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="yearEstablished">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="4"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="zipCode">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="20"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			

			<bectran:element name="contactPersonEmail">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="200"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>			
			
			
		</bectran:sequence>
	</bectran:complexType>
	
	<bectran:complexType name="sales-person-info-type">
		<bectran:sequence>
			<bectran:element name="firstName">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="100" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="lastName">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="100" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
			<bectran:element name="internalCode">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="100" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>
		</bectran:sequence>
	</bectran:complexType>
	
	<bectran:complexType name="operations-unit-info-type">
		<bectran:sequence>
			<bectran:element name="opsUnitName">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
