				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="fax">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="40"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
