			<bectran:element name="bectranCustomerId">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0"></bectran:minLength>
						<bectran:maxLength value="45"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="city">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="1"></bectran:minLength>
						<bectran:maxLength value="100"></bectran:maxLength>
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="contactPersonFirstName">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="100" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="contactPersonLastName">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="100" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="contactPersonPhone">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="40" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="contactPersonTitle">
				<bectran:simpleType>
					<bectran:restriction base="bectran:string">
						<bectran:minLength value="0" />
						<bectran:maxLength value="30" />
					</bectran:restriction>
				</bectran:simpleType>
			</bectran:element>

			<bectran:element name="countryId">
